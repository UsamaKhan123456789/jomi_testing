{
  "version": 2,
  "builds": [
    {
      "src": "api/article/[articleId].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/article/([^/]+)",
      "dest": "/api/article/[articleId]?articleId=$1"
    },
    {
      "src": "/api/test",
      "dest": "/api/test"
    }
  ]
}
